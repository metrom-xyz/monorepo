{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": []
        },
        "build-storybook": {
            "dependsOn": ["^build"],
            "outputs": []
        },
        "test": {
            "dependsOn": ["^build"],
            "inputs": [
                "src/**/*.{ts,tsx}",
                "tests/**/*.{ts,tsx}",
                "src/**/*.test.{ts,tsx}"
            ]
        },
        "lint": {},
        "format": {}
    }
}
