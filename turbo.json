{
    "$schema": "https://v2-8-9.turborepo.dev/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": []
        },
        "build-storybook": {
            "dependsOn": ["^build"],
            "outputs": []
        },
        "test": {
            "dependsOn": ["^build"],
            "inputs": [
                "src/**/*.{ts,tsx}",
                "tests/**/*.{ts,tsx}",
                "src/**/*.test.{ts,tsx}"
            ]
        },
        "lint": {},
        "format": {}
    }
}
