{
    "type": "module",
    "private": true,
    "name": "croc-subgraph",
    "scripts": {
        "prepare:deploy": "tsx ./scripts/prepare-deploy.ts",
        "codegen": "graph codegen",
        "create:local": "graph create metrom-croc-scroll --node http://localhost:8020",
        "deploy:local": "pnpm prepare:deploy scroll && pnpm codegen && graph deploy metrom-croc-scroll --node http://localhost:8020 --ipfs http://localhost:5001 -l 0.0.1",
        "deploy:scroll": "pnpm prepare:deploy scroll && pnpm codegen && graph deploy metrom-croc-scroll -l 0.0.21",
        "lint": "eslint .",
        "format": "eslint --fix ."
    },
    "dependencies": {
        "@0xgraph/cli": "^0.2.1",
        "@graphprotocol/graph-cli": "^0.96.0",
        "@graphprotocol/graph-ts": "^0.38.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.23.0",
        "@types/mustache": "^4.2.5",
        "@types/node": "^22.13.13",
        "eslint": "^9.23.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-prettier": "^5.2.5",
        "mustache": "^4.2.0",
        "prettier": "^3.5.3",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.28.0"
    }
}
